{
  "name": "newsdash",
  "description": "A news dashboard inspired by iGoogle and Netvibes",
  "version": "0.6.4",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "type": "module",
  "keywords": [
    "feedreader",
    "news",
    "rss",
    "webapp"
  ],
  "author": {
    "name": "buzz",
    "url": "https://github.com/buzz"
  },
  "homepage": "https://github.com/buzz/newsdash",
  "repository": {
    "type": "git",
    "url": "https://github.com/buzz/newsdash.git"
  },
  "scripts": {
    "build": "pnpm client:build && pnpm server:build",
    "client:build:visualize-analyze": "BUNDLE_ANALYZE=1 pnpm client:build",
    "client:build": "pnpm --filter @newsdash/client build",
    "client:dev": "pnpm --filter @newsdash/client dev",
    "lint": "eslint .",
    "server:dev": "pnpm --filter @newsdash/server dev",
    "server:start": "pnpm --filter @newsdash/server start",
    "start": "pnpm server:start",
    "type-check": "tsc -p ."
  },
  "eslintIgnore": [
    "/packages/client/dist"
  ],
  "devDependencies": {
    "@eslint/compat": "^1.0.3",
    "@eslint/js": "^9.4.0",
    "eslint": "^9.4.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-redux": "^4.0.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "globals": "^15.4.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  }
}
